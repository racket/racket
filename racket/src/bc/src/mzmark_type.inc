     live later. */
  a = (void **)s + RUNSTACK_HEADER_FIELDS;
  b = (void **)s + RUNSTACK_HEADER_FIELDS + s[2];
  while (a < b) {
    *a = RUNSTACK_ZERO_VAL;
    a++;
  }
  a = (void **)s + RUNSTACK_HEADER_FIELDS + s[3];
  b = (void **)s + RUNSTACK_HEADER_FIELDS + (s[1] - RUNSTACK_HEADER_FIELDS);
  while (a < b) {
    *a = RUNSTACK_ZERO_VAL;
    a++;
  }

# ifdef GC_NO_SIZE_NEEDED_FROM_PROCS
  return 0;
# else
  return
  s[1];
# endif
#endif
}

